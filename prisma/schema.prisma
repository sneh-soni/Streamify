generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url = env("DATABASE_URL")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  imageUrl String 
  bio String?
  externalUserId String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}